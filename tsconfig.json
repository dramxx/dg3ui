{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "resolveJsonModule": true,
    "target": "es2015",
    "module": "esnext",
    "typeRoots": ["node_modules/@types"],
    "lib": ["es2017", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "strictNullChecks": false,
    "baseUrl": ".",
    "paths": {
      "@dg3/app-menu": ["libs/app-menu/src/index.ts"],
      "@dg3/breadcrumb-navigation": ["libs/breadcrumb-navigation/src/index.ts"],
      "@dg3/components": ["libs/components/src/index.ts"],
      "@dg3/charts": ["libs/charts/src/index.ts"],
      "@dg3/graphql": ["libs/graphql/src/index.ts"],
      "@dg3/filter-create-dialog": ["libs/filter-create-dialog/src/index.ts"],
      "@dg3/report": ["libs/report/src/index.ts"],
      "@dg3/status-card": ["libs/status-card/src/index.ts"],
      "@dg3/time-filter": ["libs/time-filter/src/index.ts"],
      "@dg3/types": ["libs/types/src/index.ts"],
      "@dg3/user-settings": ["libs/user-settings/src/index.ts"],
      "@dg3/utils": ["libs/utils/src/index.ts"],
      "@dg3/map": ["libs/map/src/index.ts"],
      "@dg3/top-bar": ["libs/top-bar/src/index.ts"],
      "@dg3/schema": ["libs/schema/src/index.ts"],
      "@dg3/graph-visualization": ["libs/graph-visualization/src/index.ts"],
      "@dg3/graphiql": ["libs/graphiql/src/index.ts"],
      "@dg3/icons": ["libs/icons/src/index.ts"],
      "@dg3/reports-window": ["libs/reports-window/src/index.ts"],
      "@dg3/app-logo": ["libs/app-logo/src/index.ts"],
      "@dg3/pages": ["libs/pages/src/index.ts"],
      "@dg3/endpoints": ["libs/endpoints/src/index.ts"],
      "@dg3/table": ["libs/table/src/index.ts"],
      "@dg3/report-playground": ["libs/report-playground/src/index.ts"],
      "@dg3/module-tab-menu": ["libs/module-tab-menu/src/index.ts"],
      "@dg3/overview": ["libs/overview/src/index.ts"],
      "@dg3/unity-player": ["libs/unity-player/src/index.ts"],
      "@dg3/import-sort": ["libs/import-sort/src/index.ts"],
      "@dg3/detail": ["libs/detail/src/index.ts"],
      "@dg3/saved-filters-window": ["libs/saved-filters-window/src/index.ts"],
      "@dg3/import-unsap-dialog": ["libs/import-unsap-dialog/src/index.ts"],
      "@dg3/import-template-dialog": [
        "libs/import-template-dialog/src/index.ts"
      ],
      "@dg3/adhoc-task-dialog": ["libs/adhoc-task-dialog/src/index.ts"],
      "@dg3/clear-filter-button": ["libs/clear-filter-button/src/index.ts"],
      "@dg3/export-dialog": ["libs/export-dialog/src/index.ts"],
      "@dg3/exported-files-window": ["libs/exported-files-window/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"],
  "include": ["./libs", "./apps"]
}
